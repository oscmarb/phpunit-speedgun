{
  "name": "oscmarb/phpunit-speedgun",
  "type": "library",
  "description": "Search for slow tests in your PHPUnit test suite",
  "keywords": [
    "PHPUnit",
    "extension",
    "hook",
    "listener",
    "slow",
    "profile",
    "speed"
  ],
  "homepage": "https://github.com/oscmarb/phpunit-speedgun",
  "license": "MIT",
  "authors": [
    {
      "name": "Óscar Martínez",
      "email": "oscmarb@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "phpunit/phpunit": "^8.0 || ^9.0"
  },
  "autoload": {
    "psr-4": {
      "Oscmarb\\SpeedGun\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Oscmarb\\SpeedGun\\Tests\\": "tests/"
    }
  }
}
